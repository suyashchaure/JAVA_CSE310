import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        double a=scan.nextDouble();
        if(a>0)
        {
            int b=(int)a;
            System.out.println(b);
        }
        else
        {
            System.out.println("Invalid Input");
        }
    }
}
